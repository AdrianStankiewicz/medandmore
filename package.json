  "name": "medandmore",
  "version": "1.0.0",
  "description": "medandmore.pl",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --output-style compressed -o css src/scss",
	"serve": "browser-sync start --server --files 'css/*.css, **/*.html'",
	"watch:css": "onchange 'src/scss' -- npm run scss",
	"start": "run-p serve watch:css"
